train_net: "models/pascal_voc/ResNet-101/rfcn_alt_opt_5step_ohem/stage1_rfcn_ohem_train.pt"

base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 80000
display: 20
average_loss: 100
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "resnet101_rfcn_ohem"
iter_size: 2
